<%= form_for(@qal) do |f| %>

<%= f.label :qal_name %>
<%= f.text_field :qal_name, class: 'form-control' %>
<br>
<%= f.label :start_date %>
<%= f.date_field :start_date, class: 'form-control' %>
<br>
<%= f.label :end_date %>
<%= f.date_field :end_date, class: 'form-control' %>
<br>
<%= f.submit %>
<% end%>